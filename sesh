let SessionLoad = 1
let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
let v:this_session=expand("<sfile>:p")
silent only
cd ~/dev/lang/model
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +1 notes.txt
badd +1 src/main.c
badd +170 src/metacz.c
badd +1 src/metacz.h
badd +100 src/interpreter.c
badd +1 src/interpreter.h
badd +1 build.c
badd +1 src/tests.c
badd +39 core/utils.h
argglobal
%argdel
$argadd notes.txt
set stal=2
edit notes.txt
set splitbelow splitright
set nosplitbelow
set nosplitright
wincmd t
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
argglobal
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
